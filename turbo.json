{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env"
  ],
  "ui": "stream",
  "tasks": {
    "clean-react": {
      "cache": false
    },
    "build": {
      "env": [
        "AUTH_GOOGLE_ID",
        "AUTH_GOOGLE_SECRET",
        "AUTH_SECRET",
        "DATABASE_URL",
        "OPENAI_API_KEY",
        "RESEND_API_KEY",
        "OPENPANEL_SECRET_KEY",
        "UPSTASH_REDIS_REST_URL",
        "UPSTASH_REDIS_REST_TOKEN",
        "NEXT_PUBLIC_OPENPANEL_CLIENT_ID",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET",
        "RESEND_API_KEY",
        "RESEND_AUDIENCE_ID",
        "UPLOADTHING_TOKEN",
        "NEXT_PUBLIC_GOOGLE_TAG_ID",
        "DISCORD_WEBHOOK_URL",
        "TRIGGER_SECRET_KEY",
        "VERCEL_AUTH_TOKEN",
        "VERCEL_TEAM_ID",
        "VERCEL_PROJECT_ID",
        "NEXT_PUBLIC_VERCEL_URL",
        "NOVU_API_KEY",
        "ZAP_WEBHOOK_URL",
        "BETTER_AUTH_SECRET",
        "BETTER_AUTH_URL"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env"
      ],
      "dependsOn": [
        "^build",
        "^db:generate",
        "clean-react"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts"
      ]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "integrations:dev": {
      "cache": false
    },
    "dev": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env"
      ],
      "dependsOn": [
        "^db:generate"
      ],
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^topo"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json"
      ]
    }
  }
}